    "scripts": {
        "build": "npx rimraf dist && npm run build:cjs",
        "build:cjs": "tsc --module commonjs --outDir dist",
        "bump": "./bump-version.sh",
        "deploy": "git add . && git commit -m 'deploy' && git push",
        "push": "yarn bump && yarn build && yarn deploy"
